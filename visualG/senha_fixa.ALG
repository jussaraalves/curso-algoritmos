Algoritmo "senha_fixa"

Var

   senha : inteiro

Inicio
      
      escreval("Digite a senha: ")
      leia(senha)
      
      enquanto senha <> 0123 faca
          escreva("Senha Invalida! Tente novamente: ")
          leia(senha)
      fimenquanto
      
      escreval("Acesso permitido! ")

Fimalgoritmo